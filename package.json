{
  "name": "next-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "up": "docker compose up -d --remove-orphans",
    "logs": "docker compose logs -ft --tail 500",
    "stop": "docker compose stop",
    "restart": "docker compose restart",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:config": "eslint --print-config .eslintrc",
    "prisma": "npx prisma",
    "studio": "npx prisma studio --browser none",
    "generate": "npx prisma generate",
    "migrate": "yarn docker:migrate && yarn generate",
    "migrate:prod": "npx prisma migrate deploy",
    "docker:dev": "yarn && NODE_OPTIONS='--inspect 0.0.0.0' yarn dev",
    "docker:migrate": "docker compose exec app npx prisma migrate dev"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.1.1",
    "@fortawesome/fontawesome-pro": "6.1.1",
    "@fortawesome/fontawesome-svg-core": "6.1.1",
    "@fortawesome/free-brands-svg-icons": "6.1.1",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/pro-duotone-svg-icons": "6.1.1",
    "@fortawesome/pro-light-svg-icons": "6.1.1",
    "@fortawesome/pro-regular-svg-icons": "6.1.1",
    "@fortawesome/pro-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.18",
    "@headlessui/react": "1.7.0",
    "@next-auth/prisma-adapter": "^1.0.4",
    "@prisma/client": "4.3.1",
    "@trpc/client": "9.27.2",
    "@trpc/next": "9.27.2",
    "@trpc/react": "9.27.2",
    "@trpc/server": "9.27.2",
    "classnames": "2.3.1",
    "formik": "2.2.9",
    "joi": "^17.6.0",
    "next": "12.3.0",
    "next-auth": "^4.10.3",
    "next-superjson": "0.0.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "3.39.2",
    "superjson": "1.9.1",
    "usehooks-ts": "2.6.0",
    "zod": "3.19.0"
  },
  "devDependencies": {
    "@types/node": "16.11.58",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "autoprefixer": "^10.4.8",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-security": "^1.5.0",
    "postcss": "^8.4.16",
    "prisma": "^4.3.1",
    "tailwindcss": "^3.1.8",
    "typescript": "4.8.2"
  }
}
